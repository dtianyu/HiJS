<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <lifecycle>
        <phase-listener>
            com.jinshanlife.web.SignCheck
        </phase-listener>
    </lifecycle>
    <name>primefaces-sentinel</name> 
    <component>
        <component-type>org.primefaces.component.SentinelMenu</component-type>
        <component-class>org.primefaces.sentinel.component.menu.SentinelMenu</component-class>
    </component>   
    <render-kit>
        <renderer>
            <component-family>org.primefaces.component</component-family>
            <renderer-type>org.primefaces.component.SentinelMenuRenderer</renderer-type>
            <renderer-class>org.primefaces.sentinel.component.menu.SentinelMenuRenderer</renderer-class>
        </renderer>
    </render-kit>   
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/register.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
            <redirect/>
        </navigation-case>        
        <navigation-case>
            <from-outcome>setting</from-outcome>
            <to-view-id>/setting.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>settingNew</from-outcome>
            <to-view-id>/settingNew.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>settingEdit</from-outcome>
            <to-view-id>/settingEdit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>settingView</from-outcome>
            <to-view-id>/settingView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>weblink</from-outcome>
            <to-view-id>/weblink.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>weblinkNew</from-outcome>
            <to-view-id>/weblinkNew.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>weblinkEdit</from-outcome>
            <to-view-id>/weblinkEdit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>weblinkView</from-outcome>
            <to-view-id>/weblinkView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>store</from-outcome>
            <to-view-id>/store.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>storeNew</from-outcome>
            <to-view-id>/storeNew.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>storeEdit</from-outcome>
            <to-view-id>/storeEdit.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>storeView</from-outcome>
            <to-view-id>/storeView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>storekind</from-outcome>
            <to-view-id>/storekind.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
